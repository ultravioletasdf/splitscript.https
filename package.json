  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && fixup",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infrared-studio/splitscript.http.git"
  },
  "keywords": [
    "http",
    "async",
    "splitscript"
  ],
  "author": "ultraviolet@infrared_studio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/infrared-studio/splitscript.http/issues"
  },
  "homepage": "https://github.com/infrared-studio/splitscript.http#readme",
  "devDependencies": {
    "@types/node": "^18.15.10",
    "ava": "^5.2.0"
  }
}
