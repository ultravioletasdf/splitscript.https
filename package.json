{
  "name": "splitscript.https",
  "version": "1.0.7",
  "description": "promise based https client for node",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
